package org.jumao.bi.entites.charts;

public class CommonBean {

	private int typeOrId = -1;

	private String name;
	
	private String value;

	public CommonBean(String name, String value) {
		this.name = name;
		this.value = value;
	}

	public CommonBean(int typeOrId, String name, String value) {
		this.typeOrId = typeOrId;
		this.name = name;
		this.value = value;
	}

	public int getTypeOrId() {
		return typeOrId;
	}

	public void setTypeOrId(int typeOrId) {
		this.typeOrId = typeOrId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
