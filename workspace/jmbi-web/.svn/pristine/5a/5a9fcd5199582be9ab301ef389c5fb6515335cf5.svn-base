package com.jumore.jmbi.common.exceptions;

import com.jumore.jmbi.enums.BaseExceptionEnum;

/**
 * 远程接口异常
 *
 * @author: Lao Yang
 * @since: 2016年6月28日  下午4:18:31
 * @history:
 */
public class RemoteGenericException extends BaseException {

    /**
     * 序列号
     */
    private static final long serialVersionUID = 2786304330101867411L;

    public RemoteGenericException(String errorCode, String errorMessage, Object... args) {
        super(errorCode, errorMessage, args);
    }

    public RemoteGenericException(String errorCode, String errorMessage, Throwable cause, Object... args) {
        super(errorCode, errorMessage, cause, args);
    }

    public RemoteGenericException(BaseExceptionEnum exceptionEnum, Object... args) {
        super(exceptionEnum, args);
    }

    public RemoteGenericException(BaseExceptionEnum exceptionEnum, Throwable cause, Object... args) {
        super(exceptionEnum, cause, args);
    }
}