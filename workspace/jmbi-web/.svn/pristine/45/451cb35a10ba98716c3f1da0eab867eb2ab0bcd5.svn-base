html,
body {
  min-height: 100%;
  min-width: 1190px;
  margin: 0;
}

html body {
  font-family: 'Microsoft YaHei', sans-serif;
  background-color: #ebeef0;
}

.jm--color-primary {
  color: #0588e6;
}

/*
  ========================================
  Links
  ========================================
*/

a {
  cursor: pointer;
  text-decoration: none;
}


/*
  ========================================
  floats
  ========================================
*/

.left {
  float: left;
}

.right {
  float: right;
}

/*
  ========================================
  Clearfix
  ========================================
*/

.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.clearfix {
  clear: both;
  *zoom: 1;
}

/************* layout ******/

.jm-body {
  min-height: 100%;
  position: relative;
  font: 14px/16px "Microsoft YaHei", sans-serif;
  color: #fff;
  background-color: #ebeef0;
}

.jm-main {
  margin-top: 60px;
  margin-left: 172px;
  padding: 0 0 34px 0;
  min-height: 500px;
  position: relative;
}

.jm-content {
  padding: 20px;
  color: #666;
}

/*
  ========================================
  Panel Header
  ========================================
*/

.panel-header {
  padding: 0 16px;
  height: 42px;
  box-sizing: border-box;
  line-height: 42px;
  font-size: 16px;
  color: rgb(131, 145, 165);
  position: relative;
}

.jm-grid-border-1px {
  border: 1px solid #d9d9d9;
}

.jm-grid-border-top-1px {
  border-top: 1px solid #d9d9d9;
}

.jm-grid-border-bottom-1px {
  border-bottom: 1px solid #d9d9d9;
}

.jm-padding-bottom-20 {
  padding-bottom: 20px;
}

.jm-padding-15X20 {
  padding: 15px 20px;
}

.jm-margin-top20 {
  margin-top: 20px;
}

.jm-margin-bottom20 {
  margin-bottom: 20px;
}

.jm-grid-box-bg-white {
  background: #fff;
}

.jm-grid-box-border-radius {
  border-radius: 4px
}

.el-row {
  margin-bottom: 20px;
}

.grid-content {
  border-radius: 4px;
  min-height: 36px;
}

/*
  ========================================
  Top Header
  ========================================
*/

.top-header {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 50000;
  width: 100%;
  height: 60px;
  color: #fff;
  background-color: #0588e6;
  min-width: 320px;
  padding-left: 20px;
}
.top-header .jm-logo {
  font-size: 18px;
  line-height: 60px;
  color: #fff;
  text-decoration:none;
}
.top-header .jm-logo img {
  width: 37px;
  height: 30px;
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
}
.welcome {
  font-size: 14px;
  padding-left: 20px;
  line-height: 60px;
}
.user-profile {
  line-height: 60px;
}
.user-profile .platform {
  font-size: 14px;
  position: relative;
  top: -2px;
}
.icon-user {
  display: inline-block;
  width: 19px;
  height: 18px;
  margin-left: 20px;
  background: url(../img/top-header/icon-user.png) no-repeat;
}
.icon-setting {
  display: inline-block;
  width: 18px;
  height: 18px;
  margin-left: 20px;
  background: url(../img/top-header/icon-setting.png) no-repeat;
}
.icon-logout {
  display: inline-block;
  width: 18px;
  height: 19px;
  margin-left: 20px;
  margin-right: 40px;
  background: url(../img/top-header/icon-logout.png) no-repeat;
}
.platform {
  position: relative;
  height: 60px;
  line-height: 1.375;
  display: inline-block;
}

.platform .dropdown {
  cursor: pointer;
}
.platform .dropdown .icon-link {
  display: inline-block;
  width: 12px;
  height: 7px;
  position: relative;
  top: -2px;
  margin-left: 10px;
  background: url(../img/icon-arrow-up.png) no-repeat;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.platform .second-nav {
  position: absolute;
  border: 1px solid #0086e9;
  right: 0;
  top: 40px;
  z-index: 6;
  background: #fff;
  overflow: hidden;
  display: none;
  width: 620px;
  padding: 22px 15px 28px 30px;
  color: #666;
}
.platform .second-nav ul {
  float: left;
  list-style: none;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
.platform .second-nav ul:first-child {
  width: 325px;
  border-right: 1px solid #dedede;
}
.platform .second-nav ul:last-child {
  width: 215px;
  margin-left: 30px;
}
.platform .second-nav ul li {
  float: left;
  width: 33.3333333%;
  box-sizing: border-box;
  margin-bottom: 15px;
}
.platform .second-nav ul p {
  margin: 0;
}
.platform .second-nav ul h6 {
  margin: 0;
  margin-bottom: 18px;
  font-size: 16px;
  font-weight: normal;
}
.platform .second-nav ul a {
  font-size: 14px;
  color: #666;
}
.platform .second-nav ul a:hover {
  color: #0588e6;
}
.platform:hover .dropdown .icon-link {
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
}
.platform:hover .second-nav {
  display: block;
}

/*
  ========================================
  Top Header
  ========================================
*/


.jm-sidebar {
  width: 172px;
  top: 60px;
  left: 0;
  z-index: 1001;
  display: block;
  min-height: 100%;
  color: #8f9aae;
  background-color: #223242;
  height: 100%;
  position: fixed;
}
.jm-sidebar .icon-item {
  position: relative;
  padding-left: 15px;
}
.jm-sidebar .icon-item::after {
  content: '';
  display: block;
  width: 22px;
  height: 20px;
  position: absolute;
  top: 50%;
  left: -20px;
  margin-top: -10px;
  background: url(../img/sidebar/icon-personal-active.png) no-repeat;
}

.jm-sidebar .el-menu-item.is-active, .jm-sidebar .el-menu-item:hover {
  color: #52c0e3;
}
