/*
  ========================================
  个人中心
  ========================================
*/

.userMsg-box{
  padding: 40px 0 0 0;
  font-size: 14px;
  color: #666666;
  min-height: 600px;
}
.userMsg-box .left{
  width: 240px;
  text-align: center;
}
.userMsg-box .left p{
  margin-top: 20px;
}
.userMsg-list{
  min-width: 600px;
  float: left;
  margin: 0;
}
.userMsg-list dt{
  float: left;
  width: 100px;
  text-align: right;
  margin-right: 20px;
  height: 30px;
  line-height: 30px;
}
.userMsg-list dd{
  padding-bottom: 20px;
  line-height: 30px;
  height: 30px;
}
.userMsg-password{
  vertical-align: middle;
}
.changePassword-btn{
  width: 100px;
  height: 30px;
  margin-left: 100px;
  text-align: center;
  border-radius: 2px;
  color: #0086e9;
  border: 1px solid #0086e9;
  background-color: white;
  cursor: pointer;
}
.changePassword-btn:hover{
  background-color: #0086e9;
  color: #ffffff;
}
.userMsg-password-box .el-dialog--small{
  width: 430px;
  height: 310px;
}
.userMsg-password-box .el-input__inner{
  width: 220px;
  height: 30px;
  border: 1px solid #cbcbcb;
}
.userMsg-password-box .el-input__inner:focus{
  border: 1px solid #0086e9;
}
.userMsg-password-box .el-dialog__header{
  height: 50px;
  line-height: 50px;
  padding: 0 20px;
  border-bottom: 1px solid #cbcbcb;
}
.userMsg-password-box .el-dialog__title{
  font-weight: normal;
  color: #2b2b2b;
}
.userMsg-password-box .el-form-item__label{
  color: #666666;
  padding: 8px 12px;
}
.userMsg-password-box .el-form-item__content{
  line-height: 30px;
}
.userMsg-password-box .el-form-item + .el-form-item{
  margin: 20px 0 0 0;
}
.userMsg-password-box .el-dialog__footer{
  padding: 0;
  text-align: center;
}
.userMsg-password-box .el-button--primary{
  width: 120px;
  height: 30px;
  padding: 6px 0;
}