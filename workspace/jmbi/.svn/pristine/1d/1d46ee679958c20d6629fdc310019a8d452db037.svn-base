/*---------物性表-开始---------*/
/*搜索框*/
.searchbox{background:#f7f7f7;width:1200px;margin:0 auto;}
.searchbox dl{padding:30px 200px;width:682px;height:71px;}
.searchbox dl dt a{float:left;width:108px;padding-right:10px;font-size: 36px;color:#ff7d27;font-family: 微软雅黑;line-height: 40px;text-decoration: none;}
.searchbox dl dd {float:left;}
.searchbox dl dd .t{width:564px;height:41px;background:url(/static/image/property/searchbox.png) no-repeat;}
.searchbox dl dd .t .word{display:block;float:left;padding:10px;width:484px;height:21px;line-height: 21px;background: none;}
.searchbox dl dd .t .go{display:block;float:left;width: 60px;height: 41px;background: none;cursor: pointer;}
.searchbox dl dd .b{height:30px;}
.searchbox dl dd .b .l{float:left;line-height:30px;}
.searchbox dl dd .b .l a{padding:0px 3px;}
.searchbox dl dd .b .r{float:right;line-height:30px;}
.searchbox dl dd .b .r .n{color: #FF0000;}
/*热门牌号、最近查看*/
.property_history dl{margin-bottom: 20px;}
.property_history dl dt{height: 30px;border-bottom: solid 1px #e8e8e8;margin-bottom: 6px;}
.property_history dl dt p{width:120px;line-height: 30px;color:#0487df;font-size: 14px;border-bottom: solid 1px #57aaec;}
.property_history dl dd{padding:8px 0px;height: 14px;line-height: 14px;}
.property_history dl dd span{display: inline-block;margin-right:5px;width:14px;height:14px;line-height:14px;color: #FFFFFF;background: #898989;text-align: center;}
.property_history dl dd .s{background: #ff6600;}
/*规格参数表*/
.paratable{width: 770px;}
.paratable dl{clear:left;}
.paratable dl dt{margin:10px 3px;padding-left:30px;height: 20px;line-height: 20px;font-size:16px;font-weight:bold;color: #0088e0;}
.paratable dl dd table{border-top:solid 1px #e0e0e0;border-left:solid 1px #e0e0e0;width: 100%;}
.paratable dl dd table td{padding:0px 10px;height:30px;line-height:30px;border-bottom:solid 1px #e0e0e0;border-right:solid 1px #e0e0e0;}
.paratable dl dd table .k{background: #fbfbfb;text-align: center;font-weight: bold;}
.paratable dl dd table .w1{width:90px;}
.paratable dl dd table .w2{width:110px;}
.paratable dl dd table .w3{width:200px;}
.paratable .spec{margin-bottom: 30px;}
.paratable .spec dt{background:url(/static/image/property/property.png) no-repeat 0 -100px;}
.paratable .para dt{background:url(/static/image/property/property.png) no-repeat 0 -145px;}
/*参数临时解决*/
.parainfo .field{font-weight:bold;background: #F7F7F7;}
.parainfo .type{font-weight:bold;background: #F7F7F7;}
/*首页*/
.property_index{padding:20px 0px;width: 1198px;margin: 0 auto;}
.property_index dt{border-bottom: solid 1px #0088de;height:30px;}
.property_index dt .l {float:left;padding-left:30px;margin:5px 0px;width: 120px;line-height: 20px;background:url(/static/image/property/property.png) no-repeat;color: #0088dc;font-size: 14px;font-family: 微软雅黑;}
.property_index dt .r {float:right;position:relative;}
.property_index dt .r a{display:block;float:left;height:30px;width:16px;padding-right:20px;line-height: 30px;text-decoration: none;}
.property_index dt .r .on{color:#0088de;}
.property_index dt .r i{position:absolute;top:25px;left:-3px;width:15px;height:6px;color:#0088dc;background:url(/static/image/property/arrow.png) no-repeat bottom;}
.property_index dd{padding:20px;display: none;min-height: 360px;}
.property_index dd ul{overflow: hidden; }
.property_index dd ul li{display:inline-block;float;left;padding:0px 40px 20px 20px;width: 240px;border-left: solid 1px #CCCCCC;margin-left:-1px;height:180px;vertical-align: top;}
.property_index dd ul li .name a{color:#0088e0;font-size:14px;line-height: 30px; }
.property_index dd ul li .material a{display:inline-block;float;left;width:110px;line-height: 30px;}
.property_index .bottom{margin-top:40px;text-align: center;}
.property_index .bottom span{display:inline-block;padding-left:30px;background:url(/static/image/property/property.png) no-repeat 0 -67px;line-height: 22px;color:#fe0000;font-size: 16px;font-weight: bold;}
/*列表页*/
.property_list{padding:20px 0px;width: 1200px;}
.property_list .l{float: left;width:870px;border:solid 1px #e8e8e8;margin-bottom: 20px;}
.property_list .l dt{border-bottom: solid 1px #0088de;height:30px;}
.property_list .l dt .title{float:left;width: 120px;line-height: 30px;text-align: center;background: #0088de;color: #FFFFFF;font-size: 14px;}
.property_list .l dt .count{float:left;margin-left:30px;line-height: 30px;}
.property_list .l dt .count .n{color:#fd7d26;}
.property_list .l dd{padding: 10px 20px;}
.property_list .l dd ul li{margin-bottom:20px;line-height: 24px;}
.property_list .l dd ul li .t{font-size: 16px;color:#1121cc;}
.property_list .l dd ul li .t a{color:#1121cc;}
.property_list .l dd ul li em{color:#fd7d26;}
.property_list .l dd .none{font-size: 14px;line-height: 20px;}
.property_list .l dd .none .t{margin: 20px 0px;}
.property_list .l dd .none .t span{color:#FF0000;}
.property_list .l dd .none .b{margin: 20px 0px;}
.property_list .l dd .none .b a{color:#1121cc;}
.property_list .r{float:right;width:222px;}
/*详情页*/
.property_detail {width: 1200px;}
.property_detail .paratable{width: 870px;}
.property_detail .t{border-bottom: solid 1px #ededed;line-height: 30px;margin-top: 20px;}
.property_detail .t i{display:block;float:left;margin-top:7px;margin-right:3px;width:16px;height:16px;background:url(/static/image/property/property.png) no-repeat 0 -175px;}
.property_detail .content{overflow: auto;margin:30px 0px;}
.property_detail .content .l{float:left;width:870px;}
.property_detail .content .l .title{margin-bottom:20px;border-bottom: solid 2px #0088de;height: 30px;line-height: 30px;}
.property_detail .content .l .title .info{float:left;width: 300px;overflow: hidden;}
.property_detail .content .l .title .manu{float:left;}
.property_detail .content .l .title .share{float:right;}
.property_detail .content .l .title .share .sl{float:left;line-height: 30px;}
.property_detail .content .l .title .share .sr{float:left;margin-top:6px;}
.property_detail .content .l .bottom{margin:30px auto;text-align: center;}
.property_detail .content .l .bottom .i1{padding-left:30px;background:url(/static/image/property/property.png) no-repeat 0 -208px;;}
.property_detail .content .l .bottom .i2{display:inline-block;height:30px;width:30px;padding-left:36px;background:url(/static/image/property/property.png) no-repeat 0 -248px;line-height:30px;color:#fe7e25;text-decoration: none;}
.property_detail .content .r{float:right;width:222px;}
/*打印*/
.property_print{margin: 10px auto;width: 770px;}
.property_print .top{height:180px;}
.property_print .top .l{float:left;}
.property_print .top .img{width:415px;height:64px;}
.property_print .top .p{line-height:30px;}
.property_print .top .r{float:right;}
.property_print .top .qrcode{width:150px;height: 150px;}
.property_print .title{margin-bottom:20px;border-bottom: solid 2px #0088de;height: 30px;line-height: 30px;}
.property_print .title .info{float:left;width: 300px;overflow: hidden;}
.property_print .title .manu{float:left;}
/*---------物性表-结束---------*/