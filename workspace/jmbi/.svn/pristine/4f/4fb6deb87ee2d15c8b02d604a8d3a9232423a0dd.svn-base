body {
    background-color: #e8ebed;
}

.system-management {
    background-color: #fff;
    margin-top: 30px;
    padding: 15px;
    border: 2px solid #e1e1e1;
    border-radius: 8px;
}

.system-management-header {
    margin-bottom: 30px;
    margin-left: 15px;
    margin-top: 15px;
}

.system-management-header .nav-header {
    margin-left: 0;
}

.system-management-header .nav-header>li>a {
    text-decoration: none;
    color: #767676;
    margin-right: 60px;
    line-height: 1.42857143;
    padding-bottom: 5px;
    /*margin-bottom: 5px;*/
}

.system-management-header .nav-header>li.active>a,
.system-management-header .nav-header>li.active>a:focus,
.system-management-header .nav-header>li.active>a:hover {
    color: #5093e1;
    cursor: default;
    background-color: #fff;
    border: none;
    /*border-bottom-color: transparent;*/
    border-bottom: 1px solid #2f7cda;
}

.system-management-header .search-input {
    border: 1px solid #94bae8;
    line-height: normal;
    /*background: url(../img/icon-search.png) no-repeat 95% 50%;*/
    /*background-size: 15px 15px;*/
    padding-right: 25px;
}

.system-management-header .new-text {
    padding-left: 20px;
    color: #9e9e9e;
}

.system-management-header .new {
    display: inline-block;
    height: 30px;
    width: 30px;
    vertical-align: middle;
    margin-left: 5px;
    cursor: pointer;
    background-color: #c3c3c3;
    background: url(../img/icon-plus1.png) no-repeat center center;
    background-size: 100% 100%;
}

.table-content {
    padding-left: 55px;
    padding-right: 55px;
}

.table-content .family-name {
    color: #6ea2e4;
}

.table-content .department {
    color: #60c55f;
}

.table td.operation {
    width: 450px;
}

.table button {
    text-indent: -26px;
    width: 118px;
    height: 26px;
    background-color: #5093e1;
    color: #fff;
    border: none;
    border-radius: 4px;
    margin-right: 10px;
}

.table button.btn-edit {
    background: #5093e1 url(../img/icon-edit.png) no-repeat 80% 50%;
    background-size: 15px 15px;
}

.table button.btn-edit-disable {
    color: #a3a3a3;
    background: #e3e4e6 url(../img/icon-edit-disable.png) no-repeat 80% 50%;
    background-size: 15px 15px;
}

.table button.btn-stop {
    color: #a3a3a3;
    background: #e3e4e6 url(../img/icon-disable.png) no-repeat 80% 50%;
    background-size: 15px 15px;
}

.table button.btn-reset {
    text-indent: 0;
}

.table button.btn-start {
    background: #5093e1 url(../img/icon-unlock.png) no-repeat 78% 50%;
    background-size: 12px 16px;
}

.popover-content {
    text-align: center;
    margin-top: 15px;
}

.popover button {
    width: 60px;
    height: 22px;
    margin: 15px;
    border: none;
    border-radius: 4px;
    background-color: #5093e1;
    color: #fff;
}

.system-management .more-rows {
    height: 48px;
    line-height: 48px;
    border: 1px solid #e2e2e2;
    margin-left: 15px;
    margin-right: 15px;
    margin-top: 15px;
    color: #666666;
    background-color: #f6f7fa;
    border-radius: 4px;
    position: relative;
}

.system-management .go-back {
    display: none;
    width: 29px;
    height: 29px;
    position: absolute;
    top: 0;
    /*left: 0;*/
    bottom: 0;
    right: -50px;
    overflow: auto;
    margin: auto;
    background: #fff url(../img/icon-goback.png) no-repeat center center;
    background-size: 29px 29px;
}

.system-management .more-rows span {
    cursor: pointer;
}

@media (min-width: 992px) {
    .system-management .modal-lg {
        min-width: 900px;
        width: 1200px;
    }
}

.modal-backdrop {
    z-index: 50010;
}

.modal {
    z-index: 50020;
}

.form-horizontal .control-label {
    /*text-align: left;*/
    /*font-weight: normal;*/
    color: #3e3e3e;
    font-size: 14px;
    padding-right: 0;
}

.form-group .row-header {
    border-bottom: 1px solid #dddddd;
}

.department-user-selection {
    margin-bottom: 20px;
}

.form-horizontal .row-header label,
.form-horizontal span.sub-title {
    color: #0e8ce8;
}

.form-horizontal .row-header label.check-all-label {
    color: #3e3e3e;
}

.form-horizontal .form-group {
    margin-right: 15px;
}

.role-section {
    background-color: #f6f7fa;
    padding-bottom: 20px;
}

.modal-header .close {
    margin-top: 2px;
}

.system-management .modal-header {
    padding: 10px;
    padding-left: 22px;
    height: 46px;
    background-color: #f8f8f8;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

.form-horizontal label {
    color: #646464;
}

.modal .form-horizontal select,
.modal .form-horizontal input[type="text"],
.modal .form-horizontal input[type="password"] {
    max-width: 250px;
}

.btn.btn-search {
    width: 118px;
    background-color: #5093e1;
    color: #fff;
    border: none;
    border-radius: 4px;
    margin-right: 10px;
}

.modal-footer button {
    width: 200px;
    height: 40px;
    background-color: #2f7cda;
    color: #fff;
    margin-top: 78px;
}

.system-management .modal-footer .btn-default:active,
.system-management .modal-footer .btn-default:focus,
.system-management .modal-footer .btn-default:hover,
.system-management .modal-footer .btn-default:active:focus,
.system-management .modal-footer .btn-default:active:hover {
    background-color: #2f7cda;
    color: #fff;
}

.normal-text {
    display: block;
    width: 100%;
    height: 34px;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    padding-left: 0;
}

.errorMessage {
    color: #fb3a3a;
    line-height: 22px;
    height: 22px;
}


