	margin-bottom: 4px;
}
/*订单支付*/
.order-info .info{
	height: 42px;
	line-height: 42px;
	background: #e2f1ff;
	color: #1177dc;
	font-size: 14px;
}
.order-info .info .name{
	display: inline-block;
	vertical-align: top;
	height: 42px;
	width: auto;
	padding: 0 20px;
	line-height: 42px;
	background: #1177dc;
	color: #fff;
	text-align: center;
	margin-right: 44px;
}
.order-info .info .fn-right{
	margin-right: 12px;
	cursor: pointer;
}
.order-info .info .fn-right i{
	float: right;
	position: relative;
	height: 6px;
	width: 12px;
	margin: 18px 0px 0 8px;
	transform: rotate(180deg);
}
.order-info .info .fn-right.active i{
	transform: rotate(0deg);
}
.order-info .info .fn-right i:before,.order-info .info .fn-right i:after{
	display: inline-block;
	position: absolute;
	left: 2px;
	content: " ";
	width: 0;
	height: 0;
	border: 6px solid transparent;
	border-style: solid dashed dashed dashed;
}
.order-info .info .fn-right i:before{
	top: 1px;
	border-top-color: #1177dc;
}
.order-info .info .fn-right i:after{
	top: 0px;
	border-top-color: #e2f1ff;
}
.order-info .detail{
	display: none;
	border: 1px solid #ddd;
	border-top: none;
	background: #fff;
	color: #6d6d6d;
	font-size: 14px;
}
.order-info .detail p{
	padding-left: 28px;
	line-height: 56px;
	height: 56px;
	border-bottom: 1px solid #ddd;
}
.order-info .detail p .orange{
