/*发布需求*/
select.input-txt {
	width: 162px;
	height: 34px;
	line-height: 34px;
	border: 1px solid #ddd;
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
}
.require-form{
	background: #f6f6f6;
	padding: 20px 0 0;
	margin-bottom: 20px;
}
.require-form ul{
	overflow: hidden;
}
.require-form ul hr{
	height: 1px;
	width: 96%;
	background: #ddd;
	margin: 20px auto;
}
.require-form ul li{
	display: inline-block;
	line-height: 26px;
	margin-bottom: 10px;
}
.require-form ul li label{
	cursor: pointer;
}
.require-form ul li span{
	display: inline-block;
	vertical-align: top;
	width: 100px;
	text-align: right;
}
.require-form ul li .input-txt{
	/*vertical-align: top;*/
}
.require-form ul li .input-txt.w-long{
	width: 225px;
}
.require-form ul li .input-txt.w-short{
	width: 120px;
}
.require-form ul li textarea.input-txt.w-short{
	width: 245px;
	height: 70px;
}
.require-form ul li .input-txt.w-ms{
	width: 66px;
}
.require-form ul li .input-txt.w-ml{
	width: 600px;
}.require-form ul li select.input-txt.w-ml{
	width: 600px;
}
.require-form ul li textarea.input-txt{
	width: 600px;
	height: 40px;
	resize: none;
	padding: 4px 10px;
	line-height: 18px;
	border: 1px solid #ddd;
}
.require-form ul li .btn-b{
	line-height: 26px;
	height: 28px;
	font-size: 14px;
	overflow: hidden;
	text-align: center;
}
.require-form .kinds{
	padding: 0 0 10px 0;
	margin: 0 20px 20px 20px;
	border-bottom: 1px solid #ddd;
}
.require-form label input[type=radio],.require-form label input[type=checkbox]{
	display: inline-block;
	vertical-align: middle;
	margin: -2px 8px 0 0;
}
.require-form label{
	margin-right: 20px;
}
.require-else{
	display: none;
}
.require-form ul li .wl-btn{
	width: 60px;
	vertical-align: top;
	margin-top: 1px;
	line-height: 24px;
	height: 26px;
}
.require-form ul li .wl-choose{
	display: inline-block;
	margin:10px 0px 0px 20px;
	border: 1px solid #ddd;
	padding: 10px;
	background: #fff;
	width: 190px;
	overflow: hidden;
	cursor: pointer;
}
.require-form ul li .wl-choose.active{
	background: #ff8a00;
	color: #fff;
}
.require-form ul li .wl-choose.active dt,.require-form ul li .wl-choose.active dd{
	color: #fff;
}
.require-form ul li .wl-choose img{
	display: inline-block;
	vertical-align: middle;
	margin-right: 15px;
	width: 60px;
	height: 60px;
}
.require-form ul li .wl-choose dl{
	display: inline-block;
	vertical-align: middle;
}
.require-form ul li .wl-choose dt{
	text-align: center;
	color: #1177dc;
	font-size: 16px;
	line-height: 26px;
}
.require-form ul li .wl-choose dd{
	text-align: left;
	color: #6d6d6d;
	font-size: 14px;
	line-height: 20px;
}
