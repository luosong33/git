/*我的账户*/
input[type="file"]{
	display: inline-block;
}
input[type="file"].file-btn {
	opacity: 0;
	filter: alpha(opacity=0);
	font-size: 50px;
	position: absolute;
	top: 0;
	left: 0;
}
.index-box .per-info .btns-box .btn-w{
	width: 70px;
	margin: 2px 5px;
}
.position-re{
	position: relative;
}
input[type="text"].input-txt {
	/*min-width: 136px;*/
	/*height: 30px;*/
	line-height: 30px;
	padding: 0px 8px;
	height: 30px;
	background: #fff;
	border: 1px solid #ddd;
}
.btn-b, .btn-bb, input[type="button"].btn-b, input[type="button"].btn-bb {
	display: inline-block;
	width: 140px;
	height: 38px;
	line-height: 36px;
	border: 1px solid #1177dc;
